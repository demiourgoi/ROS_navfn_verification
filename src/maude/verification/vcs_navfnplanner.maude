      (
        astar-iteration-precond(START, GOAL, CM, PM, CUR, NEXT, EXCESS, THRESHOLD, NIT)
        and NIT === 0
        and PM' === PM
      ) implies (
        astar-iteration-postcond(START, GOAL, CM, PM, CUR, NEXT, EXCESS, THRESHOLD, NIT, PM')
      ) .


